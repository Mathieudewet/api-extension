{
    "name": "vincentchalamon/api-extension",
    "description": "Behat extension for API Platform",
    "authors": [
        {
            "name": "Vincent Chalamon",
            "email": "vincentchalamon@gmail.com"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "api-platform/core": "^2.1",
        "behat/behat": "^3.4",
        "behat/mink-extension": "^2.3",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^3.0",
        "doctrine/common": "^2.8",
        "doctrine/orm": "^2.6",
        "fzaninotto/faker": "^1.7",
        "psr/container": "^1.0",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/doctrine-bridge": "^4.0",
        "symfony/http-foundation": "^4.0",
        "symfony/http-kernel": "^4.0",
        "symfony/property-info": "^4.0",
        "symfony/validator": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ApiExtension\\": "src/",
            "ApiExtension\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10",
        "phpstan/phpstan": "^0.9.2",
        "phpunit/phpunit": "^6.0"
    }
}
